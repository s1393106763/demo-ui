{
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"]
  }
}
